{
  "name": "photography-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prestart": "node generatePhotoData.js",
    "prebuild": "node generatePhotoData.js",
    "watch-photos": "onchange 'src/assets/images/**/*' -- npm run prebuild",
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' vite",
    "docker:dev": "concurrently \"npm run dev\" \"npm run watch-photos\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.70.0",
    "@fluentui/react-icons": "^2.0.311",
    "@types/react-router-dom": "^5.3.3",
    "fs-extra": "^11.3.2",
    "image-size": "^2.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-photo-album": "^3.1.0",
    "react-router-dom": "^7.9.3",
    "sharp": "^0.34.4",
    "yet-another-react-lightbox": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "onchange": "^7.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
